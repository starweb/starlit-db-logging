{
    "name": "starlit/db-logging",
    "type": "library",
    "description": "Adds logging for the starlit/db package by providing a database monolog handler.",
    "keywords": ["database", "MySQL", "PDO", "ORM", "logging", "monolog", "handler"],
    "homepage": "http://github.com/starweb/starlit-db-logging",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Starweb AB"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo_mysql": "*",
        "starlit/db": "^0.12.2",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {"Starlit\\Db\\Logging\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Starlit\\Db\\Logging\\Tests\\": "src"}
    }
}
